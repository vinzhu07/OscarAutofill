{
  "name": "Oscar Autofiller",
  "description": "Autofill Oscar!",
  "version": "0.0.5",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/buzz16.png",
      "32": "/images/buzz32.png",
      "48": "/images/buzz48.png",
      "128": "/images/buzz128.png"
    }
  },
  "icons": {
    "16": "/images/buzz16.png",
    "32": "/images/buzz32.png",
    "48": "/images/buzz48.png",
    "128": "/images/buzz128.png"
  }
}